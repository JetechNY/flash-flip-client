ed})\n    }\n\n    handleStarCard = () => {\n\n        fetch(`http://localhost:3000/cards/${this.props.card.id}`, {\n                method: 'PATCH',\n                headers: {\n                    'Content-Type': 'application/json',\n                    'Accept': 'application/json',\n                    Authorization: `Bearer ${this.props.jwt}`\n                },\n                body: JSON.stringify({...this.props.card, is_starred: !this.state.isStarred})\n            })\n            .then(resp => resp.json())\n            .then(card => {this.setState({isStarred: card.is_starred})})\n            .catch(err => console.log(err))\n\n    }\n\n    localHandleDeleteCard = () => {\n        fetch(`http://localhost:3000/cards/${this.props.card.id}`, {\n            method:'DELETE',\n            headers: {Authorization: `Bearer ${this.props.jwt}`}\n        })\n        .then(resp => resp.json())\n        .then(data => {\n            console.log(\"successfully deleted!\", data)\n            this.props.handleDeleteCard(this.props.card.id)\n        })\n        .catch(err => console.log(err))\n    }\n\n\n\n    render(){\n        return(\n            <div className=\"flash-card\">\n                <div class=\"flip-card\">\n                    <div class=\"flip-card-inner\">\n                        <div className =\"flip-card-front\">\n                            <h2>{ this.props.card.term}</h2>\n                        </div>\n                        <div className =\"flip-card-back\">\n                            <h2>{ this.props.card.definition }</h2>\n                        </div>\n                    </div>\n                </div>\n                <div className=\"flash-card-buttons\">\n                    <Button onClick={this.handleStarCard}>{this.state.isStarred ? <i className=\"star icon\" />: <i className=\"star outline icon\" />}</Button>\n                    {this.props.parentIsGameContainer ? null : <Button onClick={this.localHandleDeleteCard}><i className=\"trash icon\" /></Button> }\n                </div>\n            </div>\n        )\n    }\n\n}\n\nexport default FlashCard\n","/Users/chun/Flatiron/react/mod4-project/flash-flip-client/src/components/CategoryForm.js",[],{"ruleId":"27","severity":1,"message":"28","line":6,"column":8,"nodeType":"29","messageId":"30","endLine":6,"endColumn":15},{"ruleId":"27","severity":1,"message":"31","line":2,"column":10,"nodeType":"29","messageId":"30","endLine":2,"endColumn":14},{"ruleId":"27","severity":1,"message":"32","line":2,"column":16,"nodeType":"29","messageId":"30","endLine":2,"endColumn":27},"no-unused-vars","'FavList' is defined but never used.","Identifier","unusedVar","'Card' is defined but never used.","'CardContent' is defined but never used."]